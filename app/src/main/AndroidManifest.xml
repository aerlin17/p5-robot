        android:theme="@style/AppTheme">
        <activity android:name=".Communication.UserInteface.DeviceListActivity" />
        <activity android:name=".Communication.UserInteface.AutomaticCommunicationsActivity" />
        <activity android:name=".Communication.UserInteface.MainCommunicationsActivity" />
    </application>
</manifest>
